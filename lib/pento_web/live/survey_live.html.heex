<% else %>
  <.live_component module={DemographicLive.Form}
    id="demographic-form"
    current_user={@current_user} />
<% end %>

<section class="row">
  <Component.hero content="Hello from a Function Component">
    <div>Hello from a Function Component's slot</div>
  </Component.hero>
</section>
